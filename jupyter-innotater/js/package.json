{
  "name": "jupyter-innotater",
  "version": "0.2.1",
  "description": "An Inline Data Annotator for Jupyter",
  "author": "danlester",
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ideonate/jupyter-innotater.git"
  },
  "keywords": [
    "jupyter",
    "widgets",
    "ipython",
    "ipywidgets",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "scripts": {
    "clean": "rimraf dist/ && rimraf ../jupyter_innotater/static/",
    "cleanall": "npm run clean && rimraf node_modules/",
    "build": "webpack --mode=production",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "watch 'npm run build' lib/",
    "watch:nbextension": "webpack --watch --mode=development lib/"
  },
  "jupyterlab": {
    "extension": "lib/labplugin",
    "discovery": {
      "kernel": [
        {
          "kernel_spec": {
            "language": "^python"
          },
          "base": {
            "name": "jupyter-innotater"
          },
          "managers": [
            "pip"
          ]
        }
      ]
    }
  },
  "devDependencies": {
    "css-loader": "^3.4.2",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "rimraf": "^3.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.3",
    "watch": "^1.0.2",
    "webpack": "^4.42.0",
    "webpack-build-notifier": "^2.0.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^3.0.0",
    "@jupyter-widgets/controls": "^2.0.0",
    "lodash": "^4.17.15"
  }
}
